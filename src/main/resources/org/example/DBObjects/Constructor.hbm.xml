<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping"><hibernate-mapping>
    <class name="org.example.DBObjects.Constructor" table="constructor">
        <id name="idConstructor" type="int" column="idConstructor">
            <generator class="identity"/>
        </id>
        <property name="nameConstructor" type="string" not-null="true" unique="true" />
        <property name="nationalityConstructor" type="string" not-null="true" />
    </class>
</hibernate-mapping>
