<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping"><hibernate-mapping>
    <class name="org.example.DBObjects.Teamed" table="teamed">
        <composite-id>
            <key-property name="idDriver" column="idDriver" type="int"/>
            <key-property name="idConstructor" column="idConstructor" type="int"/>
        </composite-id>
        <property name="yearTeamed" type="string" not-null="true"/>
    </class>
</hibernate-mapping>
