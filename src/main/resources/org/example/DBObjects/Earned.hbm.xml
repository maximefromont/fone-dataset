<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping"><hibernate-mapping>
    <class name="org.example.DBObjects.Earned" table="earned">
        <composite-id>
            <key-property name="idDriver" column="idDriver" type="int"/>
            <key-property name="idRace" column="idRace" type="int"/>
        </composite-id>
        <property name="pointsEarned" type="int" not-null="true"/>
        <property name="yearEarned" type="string" not-null="true"/>
    </class>
</hibernate-mapping>
