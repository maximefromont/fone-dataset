<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping"><hibernate-mapping>
    <class name="org.example.DBObjects.Driver" table="driver">
        <id name="idDriver" type="int" column="idDriver">
            <generator class="identity"/>
        </id>
        <property name="lastnameDriver" type="string" not-null="true" />
        <property name="firstnameDriver" type="string" not-null="true" />
    </class>
</hibernate-mapping>
